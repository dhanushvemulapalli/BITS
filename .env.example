# API Settings
API_V1_STR=/api/v1
PROJECT_NAME=Healthcare Analytics

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=11520  # 8 days

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-password-here
POSTGRES_DB=healthcare_analytics

# AWS Settings
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
S3_BUCKET=your-bucket-name

# ML Model Settings
MODEL_PATH=ml/models
FEATURE_COLUMNS=["age","bmi","blood_pressure_systolic","blood_pressure_diastolic","cholesterol","smoking_status","exercise_frequency","alcohol_consumption"]

# HIPAA Compliance
DATA_ENCRYPTION_KEY=your-encryption-key-here 